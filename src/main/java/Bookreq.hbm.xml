<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.digit.Library_Management.Model.Bookreq"
		table="book_req">
		<id name="book_id" column="book_id"></id>
		<property name="bname">
			<column name="bname" />
		</property>
		<property name="author">
			<column name="author" />
		</property>
		<property name="cost">
			<column name="cost" />
		</property>
		<property name="category">
			<column name="category" />
		</property>
		<property name="status">
			<column name="status" />
		</property>
	</class>
</hibernate-mapping>